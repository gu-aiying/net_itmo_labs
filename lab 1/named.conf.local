zone "labs.itmo.loc" {
	type master;
	file "/etc/bind/zones/db.labs.itmo.loc";
	allow-transfer { none; };
};

zone "0.168.192.in-addr.arpa" {
	type master;
	file "/etc/bind/zones/db.192.168.0";
	allow-transfer { none; };
};
